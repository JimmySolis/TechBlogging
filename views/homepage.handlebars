<div class="nameSiteBorder">
<h1 class="nameSite"> TechBlogging </h1>
</div>
{{#each posts as |post|}}
<section>
    <div class="card">
       <h3>{{title}}</h3>
    </div> 
    <div class="card-body">
       <h5>{{content}}</h5>
    </div>
    <div>
      <p class="username-poststamp">Post by {{user.username}} on {{format_date created_at}}</p>
    </div>
</section>
{{/each}}
